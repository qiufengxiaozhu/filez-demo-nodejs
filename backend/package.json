{
  "name": "filez-demo-backend",
  "version": "1.0.0",
  "description": "Filez Demo Backend - Node.js + Koa + TypeScript",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "seed": "ts-node src/database/Seed.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/database/DataSource.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/database/DataSource.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/database/DataSource.ts",
    "cev": "cev -p LS config/custom-environment-variables.json"
  },
  "keywords": [
    "filez",
    "document-management",
    "koa",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/multer": "^3.0.2",
    "@koa/router": "^13.1.0",
    "@northscaler/config-custom-environment-variables-generator": "~1.4.0",
    "config": "^3.3.12",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-session": "^6.4.0",
    "koa2-swagger-ui": "^5.10.0",
    "multer": "2.0.2",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/config": "^3.3.5",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-session": "^6.4.5",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__multer": "^2.0.7",
    "@types/koa__router": "^12.0.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
